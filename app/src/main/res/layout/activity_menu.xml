<?xml version="1.0" encoding="utf-8"?>
<layout>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuActivity"
    android:background="@color/white">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias=".3"
        android:text="Home"
        android:fontFamily="@font/segoeui"
        android:textSize="24dp"
        android:id="@+id/homeTxt"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/homeTxt"
        android:layout_marginTop="15dp"
        android:text="Profile"
        android:fontFamily="@font/segoeui"
        android:textSize="24dp"
        android:id="@+id/proTxt"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/proTxt"
        android:layout_marginTop="15dp"
        android:text="My Cart"
        android:fontFamily="@font/segoeui"
        android:textSize="24dp"
        android:id="@+id/cartTxt"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cartTxt"
        android:layout_marginTop="15dp"
        android:text="Favorite"
        android:fontFamily="@font/segoeui"
        android:textSize="24dp"
        android:id="@+id/favTxt"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/favTxt"
        android:layout_marginTop="15dp"
        android:text="My Orders"
        android:fontFamily="@font/segoeui"
        android:textSize="24dp"
        android:id="@+id/ordTxt"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ordTxt"
        android:layout_marginTop="15dp"
        android:text="Language"
        android:fontFamily="@font/segoeui"
        android:textSize="24dp"
        android:id="@+id/langTxt"/>

    <TextView
        android:id="@+id/seTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/segoeui"
        android:text="Settings"
        android:textSize="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/langTxt" />
    <ImageView
        android:id="@+id/backCross"
        android:layout_width="29dp"
        android:layout_height="29dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="20dp"
        android:src="@drawable/cencel"/>



</androidx.constraintlayout.widget.ConstraintLayout>
</layout>